{
  "name": "postcss-js",
  "version": "0.1.3",
  "description": "PostCSS for React Inline Styles, Radium, Free Style and other CSS-in-JS",
  "keywords": [
    "postcss",
    "postcss-runner",
    "js",
    "inline",
    "react",
    "css"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "postcss/postcss-js",
  "dependencies": {
    "camelcase-css": "^1.0.1",
    "postcss": "^5.2.6"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-postcss": "^2.0.2",
    "jest": "^18.0.0"
  },
  "scripts": {
    "test": "jest && eslint *.js test/*.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "rules": {
      "quote-props": "off"
    },
    "env": {
      "jest": true
    }
  }
}
